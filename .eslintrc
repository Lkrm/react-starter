{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "import/no-cycle": 0,
    "jsx-a11y/label-has-for": [ 2, {
      "required": {
        "some": [ "nesting", "id" ]
      }
    }],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "camelcase": ["error", { "ignoreDestructuring": true, "properties": "never"}],
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "linebreak-style": 0
  }
}
